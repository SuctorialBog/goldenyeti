{
  "name": "Blank Gulp Starter",
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^0.9.1",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "coffeeify": "~0.6.0",
    "connect": "~2.14.3",
    "gulp": "~3.6.0",
    "gulp-bower-files": "^0.2.4",
    "gulp-changed": "~0.3.0",
    "gulp-file-include": "^0.3.1",
    "gulp-imagemin": "~0.1.5",
    "gulp-livereload": "~1.2.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-sass": "^0.7.1",
    "gulp-uglify": "^0.3.0",
    "gulp-util": "~2.2.14",
    "hbsfy": "~1.3.2",
    "pretty-hrtime": "~0.2.1",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1"
  },
  "dependencies": {
    "jquery": "~1.11.1"
  }
}
